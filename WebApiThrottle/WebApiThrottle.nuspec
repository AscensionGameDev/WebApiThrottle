<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <version>$version$</version>
    <authors>Ascension Game Dev, Stefan Prodan</authors>
    <owners>Ascension Game Dev</owners>
    <id>AscensionGameDev.WebApiThrottle</id>
    <title>ASP.NET Web API Rate Limiter</title>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>WebApiThrottle message handler and action filter are designed for controlling the rate of requests that clients can make to an API based on IP address, client API key and request route. WebApiThrottle works with ASP.NET Web API hosted is IIS, Owin with IIS and Owin self-hosted.</description>
    <releaseNotes>If you are looking for the ASP.NET Core version please use AspNetCoreRateLimit package</releaseNotes>
    <copyright>Stefan Prodan 2013-2016</copyright>
    <tags>ASP.NET Web API, Owin, throttling, rate limits</tags>
    <projectUrl>https://github.com/AscensionGameDev/WebApiThrottle</projectUrl>
    <license type="expression">MIT</license>
    <repository type="git" url="https://github.com/AscensionGameDev/WebApiThrottle.git" branch="master" />
    <language>en-US</language>
    <dependencies>
      <group targetFramework="net45">
        <dependency id="Microsoft.AspNet.WebApi.Core" version="5.2.7" />
        <dependency id="Microsoft.Owin" version="4.1.0" />
        <dependency id="Newtonsoft.Json" version="12.0.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\WebApiThrottle.*" target="lib\net45" />
  </files>
  </package>
